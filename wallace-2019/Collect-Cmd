#root 后面是前缀，locations会拼接在一起:/home/wallace/nginx/static/
server {
	listen 6666;
	server_name 192.168.1.236;
	access_log /home/wallace/nginx/logs/default.log;
	location /static/ {
		root /home/wallace/nginx/;
		index index.html;
		autoindex on;
	}
}

